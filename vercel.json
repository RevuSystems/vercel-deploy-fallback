{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        { "type": "host", "value": "www.revusystems.ca" }
      ],
      "destination": "https://revusystems.ca",
      "permanent": true
    }
  ]
}