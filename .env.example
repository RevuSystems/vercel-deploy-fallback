# ReVu Systems Deployment Configuration
# Copy this file to .env and fill in the values

# Required for webhook deployment (primary method)
VERCEL_WEBHOOK_URL=https://api.vercel.com/v1/integrations/deploy/prj_xxxxxxxxxxxx

# Required for fallback API deployment methods
VERCEL_TOKEN=your_vercel_token_here

# ReVu Systems API Keys (not required for deployment)
OPENAI_API_KEY=sk-xxxxxxxxxxxx
CLAUDE_API_KEY=sk-xxxxxxxxxxxx
STRIPE_SECRET_KEY=sk_xxxxxxxxxxxx
